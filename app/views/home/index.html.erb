<h1>Home#index</h1>
<p>Find me in app/views/home/index.html.erb</p>
<ul>
  <li><%= link_to "Exercises", exercises_path %>
  <li><%= link_to "Workouts", workouts_path %>
</ul>

<% if user_signed_in? %>
  <h2>Circle Memberships</h2>
  <% memberships = Circle.memberships_for_user(current_user) %>
  <%= "Not a member of any circles!" if memberships.empty? %>
  <% memberships.each do |circle| %>
    <%= circle.name %>
  <% end %>
<% end %>
